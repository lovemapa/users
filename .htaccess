RewriteEngine On

RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]
RewriteRule ^ index.html [L]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.getithomenow.com/$1 [R,L]

