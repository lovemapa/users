RewriteEngine On

RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{SERVER_PORT} 80
#RewriteRule ^index\.html$ - [L]
#RewriteRule . /index.html [L]
RewriteRule ^(.*)       /index.html/#/$1
RewriteRule ^(.*)$ https://www.getithomenow.com/$1 [R,L]
